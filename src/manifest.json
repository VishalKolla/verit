{
    "manifest_version": 2,
    "name": "URL Finder",
    "version": "1.0",
    "description": "Finds all URLs on a webpage",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
